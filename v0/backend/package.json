{
  "name": "travelback",
  "version": "1.0.0",
  "description": "AI-powered backend API that processes trip photos and generates narrative summaries",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:setup": "tsx src/database/setup.ts",
    "db:migrate": "tsx src/database/migrate.ts",
    "cleanup:stuck-trips": "tsx src/scripts/cleanup-stuck-trips.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "travel",
    "photos",
    "ai",
    "openai",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^5.1.0",
    "pg": "^8.11.3",
    "dotenv": "^17.2.3",
    "multer": "^2.0.2",
    "exifr": "^7.1.3",
    "sharp": "^0.34.5",
    "openai": "^6.9.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@vitest/ui": "^4.0.9",
    "supertest": "^7.1.3",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vitest": "^4.0.9"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
