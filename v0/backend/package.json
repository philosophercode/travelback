{
  "name": "travelback",
  "version": "1.0.0",
  "description": "AI-powered backend API that processes trip photos and generates narrative summaries",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:setup": "tsx src/database/setup.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "travel",
    "photos",
    "ai",
    "openai",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "exifr": "^7.1.3",
    "sharp": "^0.33.2",
    "openai": "^4.24.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/multer": "^1.4.11",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@vitest/ui": "^1.0.0",
    "supertest": "^7.1.3",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
